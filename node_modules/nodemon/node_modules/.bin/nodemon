#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jrodrigues/Desktop/joshua/fastify/node_modules/.pnpm/nodemon@2.0.19/node_modules/nodemon/bin/node_modules:/mnt/c/Users/jrodrigues/Desktop/joshua/fastify/node_modules/.pnpm/nodemon@2.0.19/node_modules/nodemon/node_modules:/mnt/c/Users/jrodrigues/Desktop/joshua/fastify/node_modules/.pnpm/nodemon@2.0.19/node_modules:/mnt/c/Users/jrodrigues/Desktop/joshua/fastify/node_modules/.pnpm/node_modules:/mnt/c/Users/jrodrigues/Desktop/joshua/fastify/node_modules:/mnt/c/Users/jrodrigues/Desktop/joshua/node_modules:/mnt/c/Users/jrodrigues/Desktop/node_modules:/mnt/c/Users/jrodrigues/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/jrodrigues/Desktop/joshua/fastify/node_modules/.pnpm/nodemon@2.0.19/node_modules/nodemon/bin/node_modules:/mnt/c/Users/jrodrigues/Desktop/joshua/fastify/node_modules/.pnpm/nodemon@2.0.19/node_modules/nodemon/node_modules:/mnt/c/Users/jrodrigues/Desktop/joshua/fastify/node_modules/.pnpm/nodemon@2.0.19/node_modules:/mnt/c/Users/jrodrigues/Desktop/joshua/fastify/node_modules/.pnpm/node_modules:/mnt/c/Users/jrodrigues/Desktop/joshua/fastify/node_modules:/mnt/c/Users/jrodrigues/Desktop/joshua/node_modules:/mnt/c/Users/jrodrigues/Desktop/node_modules:/mnt/c/Users/jrodrigues/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nodemon.js" "$@"
else
  exec node  "$basedir/../../bin/nodemon.js" "$@"
fi
